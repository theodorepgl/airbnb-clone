<%= form_with model: @listing do |f| %>
	<%= f.label :title %>
	<%= f.text_field :title%>
	<br>
	<%= f.label :photos%>
	<%= f.file_field :photos, multiple: true%>
	<br>
	<%= f.label :description%>
	<%= f.text_field :description%>
	<br>
	<%= f.label :property_type%>
	<%= f.text_field :property_type%>
	<br>
	<%= f.label :location%>
	<%= f.text_field :location%>	
	<br>
	<%= f.label :price%>
	<%= f.text_field :price%>
	<br>
	<%= f.label :rooms%>
	<%= f.number_field :rooms%>
	<br>
	<%= f.label :capacity%>
	<%= f.number_field :capacity%>
	<br>
	<%= f.label :available%>
	<%= f.check_box :available%>
	<br>

	
	<% if @listing.new_record? %>
			<%= f.submit'Create' %>
	<% else %>
			<%= f.submit 'Update'%>
	<% end %>
<%end%>
<br>
